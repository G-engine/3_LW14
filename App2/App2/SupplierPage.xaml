<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App2.SupplierPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Product page" Clicked="Button_Product_Clicked"/>
            <Label x:Name="text" HorizontalOptions="Center"
                   VerticalOptions="CenterAndExpand"/>
            <Editor x:Name="editor" BackgroundColor="Aqua" />
            <Button Text="Get suppliers" Clicked="Button_Clicked"/>
            <Button Text="Add supplier" Clicked="Button_Add_Clicked"/>
            <Button Text="Update supplier" Clicked="Button_Update_Clicked"/>
            <Button Text="Delete supplier" Clicked="Button_Delete_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>